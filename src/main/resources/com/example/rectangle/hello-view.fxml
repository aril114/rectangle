<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<VBox alignment="CENTER" prefHeight="401.0" prefWidth="401.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rectangle.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Rectangle fx:id="rectangleForm" arcHeight="5.0" arcWidth="5.0" fill="#1f93ff" height="200.0" stroke="BLACK" strokeType="INSIDE" width="200.0" />

    <Label fx:id="sizesL" text="размеры" />
   <Label fx:id="errorL" />
   <GridPane prefHeight="42.0" prefWidth="361.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <TextField fx:id="widthTF" prefHeight="25.0" prefWidth="100.0" GridPane.rowIndex="1">
            <GridPane.margin>
               <Insets left="10.0" right="10.0" />
            </GridPane.margin></TextField>
         <TextField fx:id="heightTF" prefHeight="25.0" prefWidth="136.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
               <Insets left="10.0" right="10.0" />
            </GridPane.margin></TextField>
          <Button onAction="#onSetButtonClick" text="задать" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <GridPane.margin>
               <Insets left="10.0" />
            </GridPane.margin></Button>
         <Label text="ширина">
            <GridPane.margin>
               <Insets left="10.0" />
            </GridPane.margin>
         </Label>
         <Label text="высота" GridPane.columnIndex="1">
            <GridPane.margin>
               <Insets left="10.0" />
            </GridPane.margin>
         </Label>
      </children>
   </GridPane>
</VBox>
